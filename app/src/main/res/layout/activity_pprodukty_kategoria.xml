<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.michal.siema.Pprodukty_kategoria"
    android:background="@drawable/images1">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView77"
        android:textSize="30dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="400dp"
        android:id="@+id/listView4"
        android:layout_marginBottom="80dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Spinner
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:id="@+id/spinner5"
        android:layout_alignBottom="@+id/editText13"
        android:layout_toRightOf="@+id/editText13"
        android:layout_toEndOf="@+id/editText13"
        android:layout_marginLeft="38dp"
        android:layout_marginStart="38dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="phone"
        android:ems="10"
        android:id="@+id/editText11"
        android:layout_alignBottom="@+id/spinner6"
        android:layout_toLeftOf="@+id/spinner6"
        android:layout_toStartOf="@+id/spinner6"
        android:layout_marginRight="47dp"
        android:layout_marginEnd="47dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText12"
        android:layout_marginBottom="38dp"
        android:layout_above="@+id/listView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText13"
        android:layout_alignTop="@+id/editText12"
        android:layout_toRightOf="@+id/editText12"
        android:layout_marginLeft="38dp"
        android:layout_marginStart="38dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Nazwa :"
        android:id="@+id/textView78"
        android:layout_above="@+id/editText12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Ilość :"
        android:id="@+id/textView79"
        android:layout_above="@+id/editText13"
        android:layout_alignLeft="@+id/editText13"
        android:layout_alignStart="@+id/editText13"
        android:layout_marginBottom="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Kategoria"
        android:id="@+id/textView80"
        android:layout_alignTop="@+id/textView81"
        android:layout_alignLeft="@+id/spinner5"
        android:layout_alignStart="@+id/spinner5"
        android:layout_marginLeft="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView81"
        android:layout_above="@+id/editText11"
        android:layout_alignLeft="@+id/editText11"
        android:layout_alignStart="@+id/editText11"
        android:layout_marginBottom="20dp"
        android:text="Stan Krytyczny" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Przenieś"
        android:id="@+id/button51"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/textView77"
        android:layout_toStartOf="@+id/textView77" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel"
        android:id="@+id/button52"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textView81"
        android:layout_alignEnd="@+id/textView81" />

    <Spinner
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:id="@+id/spinner6"
        android:layout_alignBottom="@+id/editText13"
        android:layout_alignRight="@+id/listView4"
        android:layout_alignEnd="@+id/listView4"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Przynależność"
        android:id="@+id/textView86"
        android:layout_alignTop="@+id/textView81"
        android:layout_alignLeft="@+id/spinner6"
        android:layout_alignStart="@+id/spinner6"
        android:visibility="invisible" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Usuń"
        android:id="@+id/button53"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/editText11"
        android:layout_toStartOf="@+id/editText11" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Utwórz"
        android:id="@+id/button54"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/textView80"
        android:layout_toStartOf="@+id/textView80" />

</RelativeLayout>
