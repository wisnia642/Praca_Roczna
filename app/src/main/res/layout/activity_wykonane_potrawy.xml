    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="POPRAW"
        android:id="@+id/button51"
        android:layout_alignTop="@+id/button52"
        android:layout_toLeftOf="@+id/editText13"
