    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="USUN"
        android:id="@+id/button53"
        android:layout_alignTop="@+id/button52"
        android:layout_toLeftOf="@+id/spinner5"
        android:layout_toStartOf="@+id/spinner5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DODAJ"
        android:id="@+id/button54"
        android:layout_alignTop="@+id/button51"
        android:layout_toLeftOf="@+id/editText16"
        android:layout_toStartOf="@+id/editText16" />

