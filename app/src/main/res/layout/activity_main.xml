<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="@drawable/images1">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="109dp"
        android:minWidth="330dp"
        android:minHeight="530dp"
        android:weightSum="1"
        android:layout_marginStart="109dp"
        android:id="@+id/linearLayout"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/linearLayout3">

        <TextView
            android:layout_width="353dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Nazwa:"
            android:id="@+id/textView"
            android:layout_weight="0.20"
            android:layout_marginTop="60dp"
            android:minHeight="40dp" />

        <ListView
            android:layout_width="364dp"
            android:layout_height="440dp"
            android:id="@+id/listView"
            android:layout_marginTop="5dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Suma:"
            android:id="@+id/textView2"
            android:layout_marginTop="5dp"
            android:layout_weight="0.17"
            android:minHeight="50dp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout2"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:weightSum="1">

        <Button
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:id="@+id/button1"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="75dp"
            android:layout_weight="0.15"
            android:background="@drawable/dodaj" />

        <Button
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:id="@+id/button2"
            android:layout_marginBottom="20dp"
            android:background="@drawable/plus" />

        <Button
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:id="@+id/button3"
            android:layout_marginBottom="20dp"
            android:background="@drawable/usun" />

        <Button
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:id="@+id/button4"
            android:layout_marginBottom="20dp"
            android:background="@drawable/minus" />

        <Button
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:id="@+id/button5"
            android:background="@drawable/zapis" />
    </LinearLayout>

    <FrameLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="550dp"
        android:minWidth="600dp"
        android:measureWithLargestChild="true"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/linearLayout"
        android:layout_toEndOf="@+id/linearLayout"
        android:id="@+id/linearLayout3"
        android:weightSum="1"
        android:layout_marginTop="80dp">

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button18"
            android:layout_gravity="center_horizontal|top"
            android:layout_marginRight="70dp"
            android:layout_marginTop="50dp"
            android:background="@drawable/salatki" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button19"
            android:layout_gravity="left|top"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/makarony" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button20"
            android:layout_gravity="right|top"
            android:layout_marginTop="50dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/sal" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button21"
            android:layout_gravity="left|center_vertical"
            android:layout_marginTop="80dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/wina" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button22"
            android:layout_gravity="center"
            android:layout_marginRight="70dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/piwo" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button23"
            android:layout_gravity="right|center_vertical"
            android:layout_marginTop="80dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/dodatki" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button24"
            android:layout_gravity="left|bottom"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/gazowane" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button25"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginRight="70dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/zimne" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button26"
            android:layout_gravity="right|bottom"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/soki" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button27"
            android:layout_gravity="center_horizontal|top"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="50dp"
            android:background="@drawable/ryba" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button28"
            android:layout_gravity="center"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/desery" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button29"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginLeft="75dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/kawa" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button30"
            android:layout_gravity="left|center_vertical"
            android:layout_marginBottom="50dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/fast" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button31"
            android:layout_gravity="center"
            android:layout_marginRight="70dp"
            android:layout_marginBottom="50dp"
            android:background="@drawable/pizza" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
            android:id="@+id/button32"
            android:layout_gravity="right|center_vertical"
            android:layout_marginBottom="50dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/suszi" />

        <Button
            android:layout_width="122dp"
            android:layout_height="101dp"
