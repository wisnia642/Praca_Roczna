    </FrameLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/linearLayout3"
        android:layout_toRightOf="@+id/linearLayout"
        android:layout_toEndOf="@+id/linearLayout"
        android:id="@+id/linearLayout6">

        <ListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/listView2" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout5">

        <Button
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:id="@+id/button11"
            android:layout_marginRight="30dp"
            android:background="@drawable/exit" />

        <Button
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:id="@+id/button12"
            android:layout_marginRight="30dp"
            android:background="@drawable/przerwa" />

        <Button
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:id="@+id/button13"
            android:layout_marginRight="30dp"
            android:background="@drawable/napiwek" />

        <Button
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:id="@+id/button14"
            android:layout_marginRight="30dp"
            android:background="@drawable/rabat" />

        <Button
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:id="@+id/button15"
            android:layout_marginRight="30dp"
            android:background="@drawable/anulacja" />

        <Button
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:id="@+id/button16"
            android:layout_marginRight="30dp"
            android:background="@drawable/odswierz" />

        <Button
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:id="@+id/Button17"
            android:background="@drawable/kasa" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout4"
        android:layout_alignTop="@+id/linearLayout2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">

        <Button
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:id="@+id/button6"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="75dp"
            android:background="@drawable/sala1" />

        <Button
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:id="@+id/button7"
            android:layout_marginBottom="20dp"
            android:background="@drawable/sala2" />

        <Button
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:id="@+id/button9"
            android:layout_marginBottom="20dp"
            android:background="@drawable/sala3" />

        <Button
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:id="@+id/button8"
            android:layout_marginBottom="20dp"
            android:background="@drawable/sala4" />

        <Button
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:id="@+id/button10"
            android:background="@drawable/sala5" />
